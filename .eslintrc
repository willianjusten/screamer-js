{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-extra-semi": 0,
        "no-console": 0
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended"
}